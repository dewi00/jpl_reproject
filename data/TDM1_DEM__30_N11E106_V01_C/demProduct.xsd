									<xs:documentation>Specifies the high level software revision with which the product was generated</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="processingVersionCalOldest" type="xs:string">
								<xs:annotation>
									<xs:documentation>Specifies the oldest version of calibration software contributing to the product.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="processingVersionCalLatest" type="xs:string">
								<xs:annotation>
									<xs:documentation>Specifies the latest version of calibration software contributing to the product.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="processingVersionMosOldest" type="xs:string">
								<xs:annotation>
									<xs:documentation>Specifies the oldest version of mosaicking software contributing to the product.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="processingVersionMosLatest" type="xs:string">
								<xs:annotation>
									<xs:documentation>Specifies the latest version of mosaicking software contributing to the product.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="processingVersionArch" type="xs:string">
								<xs:annotation>
									<xs:documentation>Specifies the version of archiving software.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="numberOfUsedAcquisitions" type="xs:unsignedInt">
								<xs:annotation>
									<xs:documentation>Total number of acquisitions used to generate the product.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="numberOfUsedScenes" type="xs:unsignedInt">
								<xs:annotation>
									<xs:documentation>Total number of scenes used to generate the product.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="minNumberCoverages" type="xs:unsignedInt">
								<xs:annotation>
									<xs:documentation>Minimum number of acquisitions used to generate a height value.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="maxNumberCoverages" type="xs:unsignedInt">
								<xs:annotation>
									<xs:documentation>Maximum number of acquisitions used to generate a height value.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="cartographicParameters">
								<xs:annotation>
									<xs:documentation>Specifies the cartographic parameters, e.g. reference datum.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="horizontalDatumCode" type="xs:string">
											<xs:annotation>
												<xs:documentation>Horizontal datum, i.e. WGS84  in its newest realisation WGS84-G1150.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="verticalDatumCode" type="xs:string">
											<xs:annotation>
												<xs:documentation>Vertical datum, i.e. WGS84  in its newest realisation WGS84-G1150.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="coordinateSystem" type="xs:string">
											<xs:annotation>
												<xs:documentation>GEOGRAPHIC, i.e. all values are mapped in geographic coordinates.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="processingParameter">
								<xs:annotation>
									<xs:documentation>Parameters derived from and applied to processing.</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element name="bestResolutionOnGround">
											<xs:annotation>
												<xs:documentation>Best resolution of input RawDEMs on ground in meter.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:simpleContent>
													<xs:extension base="xs:float">
														<xs:attribute name="units" use="required">
															<xs:annotation>
																<xs:documentation>meter</xs:documentation>
															</xs:annotation>
															<xs:simpleType>
																<xs:restriction base="xs:string">
																	<xs:enumeration value="m"/>
																</xs:restriction>
															</xs:simpleType>
														</xs:attribute>
													</xs:extension>
												</xs:simpleContent>
											</xs:complexType>
										</xs:element>
										<xs:element name="phaseInformationIncluded" type="xs:boolean">
											<xs:annotation>
												<xs:documentation>Interferometric phase information included in the product (false)</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="accessRegion" type="xs:string">
											<xs:annotation>
												<xs:documentation>Flag to identify certain regions, e.g. GLOBAL, POLAR, ...</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="onTopMosaic" type="xs:string">
											<xs:annotation>
												<xs:documentation>Acquisition(s) were added to a prior version of the product, e.g. additional acquisition(s) were mosaicked on top.</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="demInterpolationMethod" type="xs:string">
											<xs:annotation>
												<xs:documentation>Interpolation method applied to single spikes and wells.</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="sourceScenes">
					<xs:annotation>
						<xs:documentation>Specific information of acquisition(s) used to generate the product.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="acquisition" maxOccurs="unbounded">
								<xs:annotation>
									<xs:documentation>Description of source acquisition</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="productQuality">
					<xs:annotation>
						<xs:documentation>Summarizes the image and data quality. Statistics are based on best resolution variant, e.g. 0.4 arcsec</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="qualityInspection" type="xs:string">
								<xs:annotation>
									<xs:documentation>MCP processor internal quality control result, e.g. APPROVED, LIMITED_APPROVAL, ...</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="qualityRemark" type="xs:string">
								<xs:annotation>
									<xs:documentation>MCP processor quality remark, e.g. NONE, small_PU_error, ...</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="availabilityOfSrtm" type="xs:boolean">
								<xs:annotation>
									<xs:documentation>Srtm available for validation (true/false)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="diffToSrtmMean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Mean difference to SRTM C-Band DEM</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="units" use="required">
												<xs:annotation>
													<xs:documentation>meter</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="m"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="diffToSrtmStd" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Standard deviation of difference to SRTM C-Band DEM</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="units" use="required">
												<xs:annotation>
													<xs:documentation>meter</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="m"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="diffToSrtm90Percent" minOccurs="0">
								<xs:annotation>
									<xs:documentation>90 Percent of differences to SRTM C-Band DEM are within this value</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="units" use="required">
												<xs:annotation>
													<xs:documentation>meter</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="m"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="availabilityOfIcesat" type="xs:boolean">
								<xs:annotation>
									<xs:documentation>ICESat validation points available (true/false)</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="diffToIcesatMean" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Mean difference to ICESat validation points</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="units" use="required">
												<xs:annotation>
													<xs:documentation>meter</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="m"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="diffToIcesatStd" minOccurs="0">
								<xs:annotation>
									<xs:documentation>Standard deviation of difference to ICESat validation points</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="units" use="required">
												<xs:annotation>
													<xs:documentation>meter</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="m"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="diffToIcesat90Percent" minOccurs="0">
								<xs:annotation>
									<xs:documentation>90 Percent of differences to ICESat validation points are within this value</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="units" use="required">
												<xs:annotation>
													<xs:documentation>meter</xs:documentation>
												</xs:annotation>
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="m"/>
													</xs:restriction>
												</xs:simpleType>
