					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- reference nodes: -->
	<xs:element name="acquisition">
		<xs:annotation>
			<xs:documentation>description of source scene(s)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="acquisitionItemId" type="xs:unsignedInt">
					<xs:annotation>
						<xs:documentation>ID of acqusition, e.g. 1023456</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="beamId" type="xs:string">
					<xs:annotation>
						<xs:documentation>Beam ID, e.g. tanDEM_a1_070</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="orbitDirection" type="xs:string">
					<xs:annotation>
						<xs:documentation>Orbit direction, i.e. ascending, descending</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="orbitNumber" type="xs:unsignedInt">
					<xs:annotation>
						<xs:documentation>Number of absolute orbit</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="sceneList">
					<xs:annotation>
						<xs:documentation>RawDEM number within the acquisition</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="sceneIndex" type="xs:unsignedInt" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="rawDEMList">
					<xs:annotation>
						<xs:documentation>RawDEM Identifier: "acquisitionItemId"_"scene number"_"software revision", e.g. 1023456_03_OP_ITP_01</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="rawDEMId" type="xs:string" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="acquisitionStartTimeList">
					<xs:annotation>
						<xs:documentation>RawDEM start time</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="acquisitionStartTime" type="xs:dateTime" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="incidenceAngleCenterList">
					<xs:annotation>
						<xs:documentation>RawDEM incidence angle at scene center in degrees</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="incidenceAngleCenter" type="xs:float" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="heightOfAmbiguityList">
					<xs:annotation>
						<xs:documentation>RawDEM height of ambiguity</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="heightOfAmbiguity" type="xs:float" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="phaseUnwrappingTypeList">
					<xs:annotation>
						<xs:documentation>RawDEM phase unwrapping type</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="phaseUnwrappingType" type="xs:string" maxOccurs="unbounded"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="sceneCornerCoordList">
					<xs:annotation>
						<xs:documentation>RawDEM scene corner coordinates</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="sceneCornerCoord" maxOccurs="unbounded">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="upperLeftLongitude" type="xs:float">
											<xs:annotation>
												<xs:documentation>Geographical longitude positive towards east in degrees</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="upperLeftLatitude" type="xs:float">
											<xs:annotation>
												<xs:documentation>Geographical latitude positive towards north in degrees</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="upperRightLongitude" type="xs:float">
											<xs:annotation>
												<xs:documentation>Geographical longitude positive towards east in degrees</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="upperRightLatitude" type="xs:float">
											<xs:annotation>
												<xs:documentation>Geographical latitude positive towards north in degrees</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="lowerRightLongitude" type="xs:float">
											<xs:annotation>
												<xs:documentation>Geographical longitude positive towards east in degrees</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="lowerRightLatitude" type="xs:float">
											<xs:annotation>
												<xs:documentation>Geographical latitude positive towards north in degrees</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="lowerLeftLongitude" type="xs:float">
											<xs:annotation>
												<xs:documentation>Geographical longitude positive towards east in degrees</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="lowerLeftLatitude" type="xs:float">
											<xs:annotation>
												<xs:documentation>Geographical latitude positive towards north in degrees</xs:documentation>
											</xs:annotation>
										</xs:element>
									</xs:sequence>
									<xs:attribute name="scene" type="xs:unsignedInt" use="required"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="num" type="xs:unsignedInt" use="required">
				<xs:annotation>
					<xs:documentation>Consecutive number of acquisitions, i.e. 1, 2, 3 , ...</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="imageDataInfo">
		<xs:annotation>
			<xs:documentation>Description of data layer(s)</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="pixelValueID" type="xs:string">
					<xs:annotation>
						<xs:documentation>Text describing layer content (e.g. HEIGHT, RADAR_AMPLITUDE_MEAN, ...)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="imageDataFormat" type="xs:string">
					<xs:annotation>
						<xs:documentation>Data file format (e.g. GeoTIFF)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="imageDataType" type="xs:string">
					<xs:annotation>
						<xs:documentation>Data file type (e.g. BYTE, UNSIGNED_INTEGER, FLOAT)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="imageDataDepth">
					<xs:annotation>
						<xs:documentation>Data file depth in bits (e.g. 8, 16, 32) </xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:unsignedInt">
								<xs:attribute name="units" use="required">
									<xs:annotation>
										<xs:documentation>bits per unit</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="bit"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
				<xs:element name="numberOfLayers" type="xs:unsignedInt">
					<xs:annotation>
						<xs:documentation>Layers contained in the data file, i.e. 1</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="imageStorageOrder" type="xs:string">
					<xs:annotation>
						<xs:documentation>Row by row</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="valueInvalidPixel" type="xs:float">
					<xs:annotation>
						<xs:documentation>Invalid value, e.g. -32767, 0, ...</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="rowContent" type="xs:string">
					<xs:annotation>
						<xs:documentation>LATITUDE</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="columnContent" type="xs:string">
					<xs:annotation>
						<xs:documentation>LONGITUDE</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="imageRaster">
					<xs:annotation>
						<xs:documentation>Size and spacing of image layer</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="numberOfRows" type="xs:unsignedInt"/>
							<xs:element name="numberOfColumns" type="xs:unsignedInt"/>
							<xs:element name="rowSpacing">
								<xs:annotation>
									<xs:documentation>Row spacing in arcsec</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="units" use="required">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="arcsec"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
							<xs:element name="columnSpacing">
								<xs:annotation>
									<xs:documentation>Column spacing in arcsec</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:simpleContent>
										<xs:extension base="xs:float">
											<xs:attribute name="units" use="required">
												<xs:simpleType>
													<xs:restriction base="xs:string">
														<xs:enumeration value="arcsec"/>
													</xs:restriction>
												</xs:simpleType>
											</xs:attribute>
										</xs:extension>
									</xs:simpleContent>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="imageDataStatistics">
					<xs:annotation>
						<xs:documentation>Statistics of image layer</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="numberOfInvalidPixels" type="xs:unsignedLong"/>
							<xs:element name="min" type="xs:float"/>
							<xs:element name="max" type="xs:float"/>
							<xs:element name="mean" type="xs:float"/>
							<xs:element name="stdDev" type="xs:float">
								<xs:annotation>
									<xs:documentation>Standard deviation</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="name" type="xs:string" use="required">
				<xs:annotation>
					<xs:documentation>Mnemonic identifying layer content (e.g. DEM, HEM, AMP, ...)</xs:documentation>
				</xs:annotation>
			</xs:attribute>
		</xs:complexType>
	</xs:element>
	<xs:element name="file">
		<xs:annotation>
			<xs:documentation>Pointer to product component</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="location">
					<xs:annotation>
						<xs:documentation>Location of product component</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="imageDataFormat" type="xs:string">
					<xs:annotation>
						<xs:documentation>Data file format (e.g. GeoTIFF)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="imageDataType" type="xs:string">
					<xs:annotation>
						<xs:documentation>Data file type (e.g. BYTE, UNSIGNED_INTEGER, FLOAT)</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="imageDataDepth" type="xs:string">
					<xs:annotation>
						<xs:documentation>Data file depth in bits (e.g. 8, 16, 32) </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="size" type="xs:long">
					<xs:annotation>
						<xs:documentation>File size in bytes</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="location">
		<xs:annotation>
			<xs:documentation>Location of product components</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="host" type="xs:string">
					<xs:annotation>
						<xs:documentation>Host defaults to: . </xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="path" type="xs:string"/>
				<xs:element name="filename" type="xs:string"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
